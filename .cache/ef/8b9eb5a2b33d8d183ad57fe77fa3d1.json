},"generated":{"line":71,"column":0}},{"source":"Handlers/KeyboardHandler.ts","name":null,"original":{"line":78,"column":0},"generated":{"line":71,"column":9}},{"source":"Handlers/KeyboardHandler.ts","name":null,"original":{"line":78,"column":0},"generated":{"line":71,"column":24}},{"source":"Handlers/KeyboardHandler.ts","name":null,"original":{"line":4,"column":0},"generated":{"line":72,"column":0}},{"source":"Handlers/KeyboardHandler.ts","name":null,"original":{"line":4,"column":0},"generated":{"line":72,"column":1}},{"source":"Handlers/KeyboardHandler.ts","name":null,"original":{"line":4,"column":0},"generated":{"line":72,"column":3}},{"source":"Handlers/KeyboardHandler.ts","name":null,"original":{"line":80,"column":0},"generated":{"line":74,"column":0}},{"source":"Handlers/KeyboardHandler.ts","name":null,"original":{"line":80,"column":0},"generated":{"line":74,"column":7}},{"source":"Handlers/KeyboardHandler.ts","name":null,"original":{"line":80,"column":0},"generated":{"line":74,"column":8}},{"source":"Handlers/KeyboardHandler.ts","name":null,"original":{"line":80,"column":0},"generated":{"line":74,"column":15}},{"source":"Handlers/KeyboardHandler.ts","name":null,"original":{"line":80,"column":15},"generated":{"line":74,"column":18}},{"source":"Handlers/KeyboardHandler.ts","name":null,"original":{"line":80,"column":0},"generated":{"line":74,"column":33}}],"sources":{"Handlers/KeyboardHandler.ts":"import { Key } from 'ts-keycode-enum';\r\nimport { SimpleEventDispatcher} from \"strongly-typed-events\";\r\n\r\nclass KeyboardHandler{\r\n    public keys = new Map<Key, KeyboardEvent>();\r\n    public onKeyDown = new SimpleEventDispatcher<KeyboardEvent>();\r\n    public onKeyUp = new SimpleEventDispatcher<KeyboardEvent>();\r\n    public onKeyPressed = new SimpleEventDispatcher<KeyboardEvent>()\r\n    //public onKeyPressed = new EventDispatcher<KeyboardEvent, Key>();\r\n    //public onKeyDown = new SimpleEventDispatcher<Key>();\r\n\r\n    constructor(){\r\n        this.addEventListners();\r\n    }\r\n\r\n\r\n    private addEventListners() :void {\r\n        document.addEventListener(\r\n            \"keydown\", \r\n            function(event){\r\n                this.doOnKeyDown(event); \r\n            }.bind(this)\r\n        );\r\n        document.addEventListener(\r\n            \"keyup\",\r\n            function(event) {\r\n                this.doOnKeyUp(event);\r\n            }.bind(this)\r\n        );\r\n        \r\n    }\r\n\r\n    private doOnKeyDown(event) : void {\r\n        let keyCode = event.keyCode\r\n\r\n        //if (this.keys.has(keyCode)) \r\n            //return;\r\n\r\n        this.keys.set(keyCode, event);\r\n        //this.onKeyDown.dispatch(keyCode as Key);\r\n        //this.onKeyPressed.dispatch(event as KeyboardEvent, keyCode as Key);\r\n        this.onKeyDown.dispatch(event as KeyboardEvent);\r\n        //console.log(\"key pressed = \" + Key[keyCode]);\r\n        //console.log(this.keys.get(Key.Enter));  \r\n    }\r\n\r\n    private doOnKeyUp(event) : void\r\n    {\r\n        let keyCode = event.keyCode\r\n\r\n        if (!this.keys.has(keyCode))\r\n            return;\r\n\r\n        this.keys.delete(keyCode);\r\n        this.onKeyUp.dispatch(event as KeyboardEvent);\r\n        //console.log(\"key released = \" + Key[keyCode]);\r\n        this.update();\r\n    }\r\n\r\n    public update(){\r\n        if(this.keys.size > 0)\r\n        this.keys.forEach((value: KeyboardEvent) => this.onKeyPressed.dispatch(value));\r\n    }\r\n\r\n    \r\n\r\n    public getKey(keyCode: Key): boolean{\r\n        return this.keys.has(keyCode) \r\n    }\r\n\r\n    public getKeyDown(keyCode: Key): boolean{\r\n        if(this.keys.has(keyCode)){\r\n        return(this.keys.get(keyCode).repeat ? false : true)\r\n    }\r\n        return false;\r\n    }\r\n\r\n}\r\n\r\nexport default KeyboardHandler;"},"lineCount":null}},"error":null,"hash":"43860b12d5960620ac94ea98aa19153c","cacheData":{"env":{}}}